<h1>Add a new project</h1>

<form action="/projects" method="post">
  <div class="form-group">
    <label for="project_title">Project name</label>
    <input id="n" name="project_title" class="form-control" type="text">

    <label for="project_volunteer">Project Volunteer</label>
    <input id="n" name="project_volunteer" class="form-control" type="text">

  </div>
  <button type="submit" class="btn btn-primary">Go!</button>
</form>
